{
  "name": "sqli-testing-framework",
  "version": "1.0.0",
  "description": "SQL injection testing and validation framework for multiple database platforms",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/cli.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint . && prettier --check .",
    "lint:fix": "eslint --fix . && prettier --write .",
    "typecheck": "tsc --noEmit",
    "docker:up": "docker compose -f docker/docker-compose.yml up -d",
    "docker:down": "docker compose -f docker/docker-compose.yml down",
    "docker:logs": "docker compose -f docker/docker-compose.yml logs -f",
    "app:start": "tsx src/app/server.ts"
  },
  "keywords": [
    "sql-injection",
    "security-testing",
    "postgresql",
    "database",
    "penetration-testing"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "express": "^5.1.0",
    "pg": "^8.16.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/express": "^5.0.2",
    "@types/node": "^22.15.29",
    "@types/pg": "^8.15.4",
    "eslint": "^9.28.0",
    "globals": "^16.2.0",
    "prettier": "^3.5.3",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1",
    "vitest": "^3.2.3"
  }
}
